<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pocketdoctor">

    <application
        android:name=".PocketDoctorApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".EditDoctorAndCashierActivity"></activity>
        <activity android:name=".CreateDoctorAndCashierActivity" />
        <activity android:name=".SearchUserAdminActivity" />
        <activity
            android:name=".FoodTrackerActivity"
            android:windowSoftInputMode="adjustNothing" />
        <activity android:name=".HomeAdmin" />
        <activity android:name=".PatientAccountActivity" />
        <activity android:name=".CreateAdminUserAccountActivity" />
        <activity android:name=".EditAdminUserAccountActivity" />
        <activity android:name=".FindDoctor" />
        <activity android:name=".ListOfDoctors" />
        <activity android:name=".Booking" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".DoctorPocketSplash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
    </application>

</manifest>